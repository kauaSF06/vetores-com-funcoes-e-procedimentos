programa {
  funcao vazio mostrar_array(){
    inteiro array[5]
    inteiro i
    escreva("----- digite um nuero para o array----\n")
    para(i = 0; i<=4; i++){
      leia(array)
      escreva("\no array numero e [", i, "] recebe[", array, "]")
    }
  }
  funcao inicio() {
    mostrar_array()
  }
}
